
package

import java.io.Serializable;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

/**
 *
 * @author Nicolas Oliva
 */
@Entity //Indica a hibernate que es una entidad para persisr
@Table(name="tb_area") //Indica el nombre de la tabla en la base de datos
@Getter @Setter @NoArgsConstructor //Anotaciones de lombok
public class Area implements Serializable
{
    @Id //Indica identificacion del objeto
    @Column(name="idArea") // Nombre de la columna
    private int idArea;
    
    private String nombre; // Si no ingreso parametro name, toma por 
    //defecto el nombre del atributo.
    
}
